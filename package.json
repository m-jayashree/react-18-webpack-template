{
  "name": "react-typescript",
  "version": "0.1.0",
  "private": true,
  "homepage": "",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "post-commit": "git update-index -g"
    }
  },
  "scripts": {
    "---------- HELPERS -------------------------------------------------------------------------------------": "",
    "generate": "node ./tools/generate.js",
    "---------- DEVELOPMENT ---------------------------------------------------------------------------------": "",
    "start": "cross-env CLIENT_ENV=development craco  start",
    "prod": "cross-env CLIENT_ENV=production craco start",
    "---------- PRODUCTION ----------------------------------------------------------------------------------": "",
    "build": "cross-env CLIENT_ENV=production craco build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "---------- TESTING -------------------------------------------------------------------------------------": "",
    "test": "cross-env CLIENT_ENV=test craco test",
    "ts": "tsc --noEmit",
    "ts:watch": "npm run ts -- --watch",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "--------------------------------------------------------------------------------------------------------": ""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^1.6.2",
    "axios-auth-refresh": "^3.3.6",
    "bootstrap": "^5.3.3",
    "classnames": "2.3.2",
    "connected-react-router": "^6.9.3",
    "dayjs": "1.11.10",
    "history": "^4.10.1",
    "jwt-decode": "^4.0.0",
    "lodash.groupby": "4.6.0",
    "moment": "^2.30.1",
    "quagga": "^0.12.1",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-bootstrap": "^2.10.4",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.1",
    "react-redux": "8.1.3",
    "react-redux-i18n": "^1.9.3",
    "react-router-dom": "5.1.2",
    "react-scripts": "^3.0.1",
    "redux": "4.2.1",
    "redux-devtools-extension": "2.13.8",
    "redux-freeze": "0.1.7",
    "redux-thunk": "2.4.2",
    "reselect": "4.1.8",
    "sass": "^1.77.8",
    "save-dev": "^0.0.1-security",
    "sjs-base-model": "2.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@craco/craco": "5.6.3",
    "@types/classnames": "2.2.9",
    "@types/history": "4.7.5",
    "@types/jest": "25.1.3",
    "@types/lodash.groupby": "4.6.6",
    "@types/node": "13.7.7",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/uuid": "7.0.0",
    "ajv": "^8.17.1",
    "cross-env": "7.0.0",
    "dart-sass": "1.25.0",
    "generate-template-files": "2.2.1",
    "gh-pages": "^6.1.1",
    "husky": "^4.3.8",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "typescript": "^4.6.2"
  }
}
